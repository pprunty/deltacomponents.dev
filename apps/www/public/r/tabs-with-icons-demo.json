{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "tabs-with-icons-demo",
  "dependencies": [
    "lucide-react"
  ],
  "registryDependencies": [
    "tabs",
    "https://deltacomponents.dev/r/tabs.json"
  ],
  "files": [
    {
      "path": "examples/tabs-with-icons-demo.tsx",
      "content": "\"use client\"\n\nimport { Lock, Settings, User } from \"lucide-react\"\n\nimport {\n  Tabs,\n  TabsContent,\n  TabsList,\n  TabsTrigger,\n} from \"@/components/ui/tabs\"\n\nexport default function TabsWithIconsDemo() {\n  return (\n    <Tabs defaultValue=\"account\" variant=\"default\" size=\"default\">\n      <TabsList>\n        <TabsTrigger value=\"account\">\n          <User className=\"h-4 w-4\" />\n          Account\n        </TabsTrigger>\n        <TabsTrigger value=\"password\">\n          <Lock className=\"h-4 w-4\" />\n          Password\n        </TabsTrigger>\n        <TabsTrigger value=\"settings\">\n          <Settings className=\"h-4 w-4\" />\n          Settings\n        </TabsTrigger>\n      </TabsList>\n      <div className=\"relative min-h-[60px]\">\n        <TabsContent value=\"account\" className=\"absolute inset-x-0 top-0\">\n          <p className=\"text-muted-foreground text-sm\">\n            Manage your account settings and preferences.\n          </p>\n        </TabsContent>\n        <TabsContent value=\"password\" className=\"absolute inset-x-0 top-0\">\n          <p className=\"text-muted-foreground text-sm\">\n            Change your password here.\n          </p>\n        </TabsContent>\n        <TabsContent value=\"settings\" className=\"absolute inset-x-0 top-0\">\n          <p className=\"text-muted-foreground text-sm\">\n            Configure your application settings.\n          </p>\n        </TabsContent>\n      </div>\n    </Tabs>\n  )\n}\n",
      "type": "registry:example"
    }
  ],
  "type": "registry:example"
}