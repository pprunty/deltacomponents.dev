  "files": [
    {
      "path": "examples/tabs-entrance-animation-demo.tsx",
      "content": "\"use client\"\n\nimport { Tabs, TabsList, TabsTrigger, TabsContent } from \"@/registry/delta-ui/delta/tabs\"\n\nexport function TabsEntranceVariantDemo() {\n  return (\n    <Tabs defaultValue=\"account\" variant=\"underline\" size=\"large\">\n      <TabsList>\n        <TabsTrigger value=\"account\">Account</TabsTrigger>\n        <TabsTrigger value=\"password\">Password</TabsTrigger>\n        <TabsTrigger value=\"settings\">Settings</TabsTrigger>\n      </TabsList>\n      <div className=\"relative min-h-[60px]\">\n        <TabsContent\n          value=\"account\"\n          className=\"absolute inset-x-0 top-0\"\n          animate\n          animateY={8}\n        >\n          <p className=\"text-sm text-muted-foreground\">Manage your account settings.</p>\n        </TabsContent>\n        <TabsContent\n          value=\"password\"\n          className=\"absolute inset-x-0 top-0\"\n          animate\n          animateY={8}\n        >\n          <p className=\"text-sm text-muted-foreground\">Change your password here.</p>\n        </TabsContent>\n        <TabsContent\n          value=\"settings\"\n          className=\"absolute inset-x-0 top-0\"\n          animate\n          animateY={8}\n        >\n          <p className=\"text-sm text-muted-foreground\">Configure your preferences.</p>\n        </TabsContent>\n      </div>\n    </Tabs>\n  )\n}\n",
      "type": "registry:example"
    }
  ]
}