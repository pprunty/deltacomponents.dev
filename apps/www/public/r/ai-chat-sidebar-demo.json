      "path": "examples/ai-chat-sidebar-demo.tsx",
      "content": "\"use client\"\n\nimport {\n  AiChatSidebar,\n  AiChatSidebarContent,\n} from \"@/registry/delta-ui/blocks/ai-chat-sidebar\"\nimport {\n  Breadcrumb,\n  BreadcrumbItem,\n  BreadcrumbLink,\n  BreadcrumbList,\n  BreadcrumbPage,\n  BreadcrumbSeparator,\n} from \"@/components/ui/breadcrumb\"\nimport { Separator } from \"@/components/ui/separator\"\nimport {\n  SidebarInset,\n  SidebarProvider,\n  SidebarTrigger,\n} from \"@/components/ui/sidebar\"\n\nexport default function AiChatSidebarDemo() {\n  return (\n    <div className=\"w-full\">\n      <div className=\"space-y-8\">\n        <div className=\"space-y-2 text-center\">\n          <h2 className=\"text-2xl font-bold\">AI Chat Sidebar Demo</h2>\n          <p className=\"text-muted-foreground\">\n            Full-featured collapsible sidebar for AI chat applications with\n            logo, recent chats, and settings.\n          </p>\n        </div>\n\n        {/* Live Demo */}\n        <div className=\"overflow-hidden rounded-lg border\">\n          <div className=\"bg-muted p-4\">\n            <h3 className=\"font-semibold\">Live Preview</h3>\n          </div>\n          <div className=\"flex h-[600px]\">\n            <SidebarProvider>\n              <AiChatSidebarContent />\n              <SidebarInset>\n                <header className=\"flex h-16 shrink-0 items-center gap-2 border-b px-4\">\n                  <SidebarTrigger className=\"-ml-1\" />\n                  <Separator orientation=\"vertical\" className=\"mr-2 h-4\" />\n                  <Breadcrumb>\n                    <BreadcrumbList>\n                      <BreadcrumbItem className=\"hidden md:block\">\n                        <BreadcrumbLink href=\"#\">AI Chat</BreadcrumbLink>\n                      </BreadcrumbItem>\n                      <BreadcrumbSeparator className=\"hidden md:block\" />\n                      <BreadcrumbItem>\n                        <BreadcrumbPage>New Conversation</BreadcrumbPage>\n                      </BreadcrumbItem>\n                    </BreadcrumbList>\n                  </Breadcrumb>\n                </header>\n                <div className=\"flex flex-1 flex-col gap-4 p-4\">\n                  <div className=\"flex flex-1 items-center justify-center\">\n                    <div className=\"text-muted-foreground text-center\">\n                      <h2 className=\"mb-2 text-2xl font-semibold\">\n                        Start a new conversation\n                      </h2>\n                      <p>\n                        Click the toggle button to collapse/expand the sidebar\n                      </p>\n                      <p className=\"mt-2 text-sm\">\n                        Try clicking on recent chats or the settings button\n                      </p>\n                    </div>\n                  </div>\n                </div>\n              </SidebarInset>\n            </SidebarProvider>\n          </div>\n        </div>\n\n        {/* Features */}\n        <div className=\"space-y-4\">\n          <h3 className=\"text-lg font-semibold\">Features</h3>\n          <div className=\"grid gap-4 md:grid-cols-2\">\n            <div className=\"rounded-lg border p-4\">\n              <h4 className=\"mb-2 font-medium\">Collapsible Design</h4>\n              <p className=\"text-muted-foreground text-sm\">\n                Toggle between full and icon-only mode with smooth animations\n              </p>\n            </div>\n            <div className=\"rounded-lg border p-4\">\n              <h4 className=\"mb-2 font-medium\">Recent Chats</h4>\n              <p className=\"text-muted-foreground text-sm\">\n                Display list of recent conversations with truncated titles\n              </p>\n            </div>\n            <div className=\"rounded-lg border p-4\">\n              <h4 className=\"mb-2 font-medium\">Custom Logo</h4>\n              <p className=\"text-muted-foreground text-sm\">\n                Includes a custom SVG logo that adapts to the theme\n              </p>\n            </div>\n            <div className=\"rounded-lg border p-4\">\n              <h4 className=\"mb-2 font-medium\">Action Buttons</h4>\n              <p className=\"text-muted-foreground text-sm\">\n                New chat and settings buttons with tooltips when collapsed\n              </p>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  )\n}\n",
      "type": "registry:example"
    }
  ]
