      "content": "\"use client\"\n\nimport React from \"react\";\nimport { FloatingObject } from \"@/registry/animations/floating-object\";\n\nexport default function FloatingObjectCompassDemo() {\n  return (\n    <div className=\"w-full max-w-3xl mx-auto space-y-6 p-4\">\n      <div className=\"text-center space-y-2\">\n        <h2 className=\"text-xl font-bold\">Random Directional Movement</h2>\n        <p className=\"text-sm text-muted-foreground\">\n          Watch the object move between 8 compass directions with unpredictable paths\n        </p>\n      </div>\n      \n      <div className=\"relative h-96 bg-card border border-border rounded-xl overflow-hidden\">\n        {/* Compass directions overlay */}\n        <div className=\"absolute inset-0 pointer-events-none\">\n          <div className=\"absolute top-4 left-1/2 transform -translate-x-1/2 text-muted-foreground text-sm font-medium\">\n            N\n          </div>\n          <div className=\"absolute top-6 right-12 transform text-muted-foreground text-xs\">\n            NE\n          </div>\n          <div className=\"absolute top-1/2 right-4 transform -translate-y-1/2 text-muted-foreground text-sm font-medium\">\n            E\n          </div>\n          <div className=\"absolute bottom-12 right-12 transform text-muted-foreground text-xs\">\n            SE\n          </div>\n          <div className=\"absolute bottom-4 left-1/2 transform -translate-x-1/2 text-muted-foreground text-sm font-medium\">\n            S\n          </div>\n          <div className=\"absolute bottom-12 left-12 transform text-muted-foreground text-xs\">\n            SW\n          </div>\n          <div className=\"absolute top-1/2 left-4 transform -translate-y-1/2 text-muted-foreground text-sm font-medium\">\n            W\n          </div>\n          <div className=\"absolute top-6 left-12 transform text-muted-foreground text-xs\">\n            NW\n          </div>\n          \n          {/* Center indicator */}\n          <div className=\"absolute top-1/2 left-1/2 transform -translate-x-1/2 -translate-y-1/2 w-1 h-1 bg-muted-foreground rounded-full opacity-30\" />\n        </div>\n\n        {/* Floating object */}\n        <FloatingObject\n          pattern=\"random-directions\"\n          intensity=\"medium\"\n          speed=\"medium\"\n          className=\"absolute top-1/2 left-1/2 transform -translate-x-1/2 -translate-y-1/2\"\n        >\n          <div className=\"relative\">\n            <div className=\"w-16 h-16 bg-gradient-to-br from-primary to-primary/70 rounded-full shadow-lg border-2 border-primary/20 flex items-center justify-center\">\n              <div className=\"text-white text-lg\">ðŸŽ¯</div>\n            </div>\n            <div className=\"absolute -top-1 -right-1 w-3 h-3 bg-yellow-400 rounded-full animate-pulse shadow-sm\" />\n          </div>\n        </FloatingObject>\n        \n        {/* Path visualization hint */}\n        <div className=\"absolute bottom-4 left-4 text-xs text-muted-foreground bg-card/80 backdrop-blur-sm rounded px-2 py-1\">\n          Generates new random paths continuously\n        </div>\n      </div>\n    </div>\n  );\n}\n",
