{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "floating-object-multiple-demo",
  "type": "registry:block",
  "registryDependencies": [
    "https://deltacomponents.dev/r/floating-object.json"
  ],
  "files": [
    {
      "path": "registry/examples/floating-object-multiple-demo.tsx",
      "content": "\"use client\"\n\nimport React from \"react\";\nimport { FloatingObjects } from \"@/registry/animations/floating-object\";\n\nexport default function FloatingObjectMultipleDemo() {\n  const objects = [\n    { emoji: \"🎨\", color: \"from-blue-500 to-blue-600\", label: \"Art\" },\n    { emoji: \"🚀\", color: \"from-purple-500 to-purple-600\", label: \"Space\" },\n    { emoji: \"⚡\", color: \"from-yellow-500 to-yellow-600\", label: \"Energy\" },\n    { emoji: \"🌟\", color: \"from-pink-500 to-pink-600\", label: \"Star\" },\n    { emoji: \"🎯\", color: \"from-green-500 to-green-600\", label: \"Target\" },\n    { emoji: \"💎\", color: \"from-cyan-500 to-cyan-600\", label: \"Diamond\" },\n  ];\n\n  return (\n    <div className=\"w-full max-w-4xl mx-auto space-y-6 p-4\">\n      <div className=\"text-center space-y-2\">\n        <h2 className=\"text-xl font-bold\">Multiple Floating Objects</h2>\n        <p className=\"text-sm text-muted-foreground\">\n          Watch objects bounce off walls and each other with realistic physics\n        </p>\n      </div>\n      \n      <div className=\"bg-gradient-to-br from-muted/30 to-muted/10 border border-border rounded-xl p-6\">\n        <FloatingObjects\n          containerWidth={600}\n          containerHeight={400}\n          intensity=\"medium\"\n          speed=\"medium\"\n          collisionEnabled={true}\n          className=\"mx-auto bg-card border border-border rounded-lg shadow-inner\"\n        >\n          {objects.map((obj, index) => (\n            <div key={index} className=\"relative group\">\n              <div className={`w-16 h-16 bg-gradient-to-br ${obj.color} rounded-full shadow-lg border-2 border-white/20 flex items-center justify-center transition-transform group-hover:scale-110`}>\n                <span className=\"text-white text-xl\">{obj.emoji}</span>\n              </div>\n              <div className=\"absolute -bottom-2 left-1/2 transform -translate-x-1/2 opacity-0 group-hover:opacity-100 transition-opacity\">\n                <div className=\"bg-black/80 text-white text-xs px-2 py-1 rounded whitespace-nowrap\">\n                  {obj.label}\n                </div>\n              </div>\n            </div>\n          ))}\n        </FloatingObjects>\n        \n        <div className=\"mt-4 text-center\">\n          <p className=\"text-xs text-muted-foreground\">\n            Hover over objects to see their labels • Collision physics enabled\n          </p>\n        </div>\n      </div>\n      \n      <div className=\"grid grid-cols-1 md:grid-cols-3 gap-4 text-sm\">\n        <div className=\"text-center p-3 bg-muted/50 rounded-lg\">\n          <div className=\"text-lg mb-1\">🏀</div>\n          <div className=\"font-medium\">Physics</div>\n          <div className=\"text-xs text-muted-foreground\">Realistic bouncing and collision detection</div>\n        </div>\n        <div className=\"text-center p-3 bg-muted/50 rounded-lg\">\n          <div className=\"text-lg mb-1\">🎮</div>\n          <div className=\"font-medium\">Interactive</div>\n          <div className=\"text-xs text-muted-foreground\">Smooth 60fps animations with wall bouncing</div>\n        </div>\n        <div className=\"text-center p-3 bg-muted/50 rounded-lg\">\n          <div className=\"text-lg mb-1\">⚙️</div>\n          <div className=\"font-medium\">Configurable</div>\n          <div className=\"text-xs text-muted-foreground\">Adjustable speed, intensity, and collision settings</div>\n        </div>\n      </div>\n    </div>\n  );\n}\n",
      "type": "registry:block"
    }
  ]
}
