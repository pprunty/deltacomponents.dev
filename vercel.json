{
  "version": 2,
  "builds": [
    {
      "src": "apps/www/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/www/$1"
    }
  ],
  "installCommand": "bun install",
  "buildCommand": "cd apps/www && bun run build",
  "devCommand": "cd apps/www && bun run dev",
  "outputDirectory": "apps/www/.next"
}